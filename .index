<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>AI Sentinel-X | Super Admin Command Center</title>
    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        :root {
            --primary-bg: #0a0a0a;
            --secondary-bg: #1a1a1a;
            --card-bg: #0f0f0f;
            --accent: #00ff88;
            --danger: #ff3366;
            --warning: #ffaa33;
            --text-primary: #ffffff;
            --text-secondary: #888888;
            --border-color: #2a2a2a;
            --success: #00ff88;
            --info: #3388ff;
        }

        body {
            font-family: 'SF Pro Display', -apple-system, BlinkMacSystemFont, sans-serif;
            background: var(--primary-bg);
            color: var(--text-primary);
            line-height: 1.6;
            overflow-x: hidden;
        }

        /* Animated Background */
        .bg-animation {
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            z-index: -1;
            background: radial-gradient(circle at 20% 50%, rgba(0, 255, 136, 0.1) 0%, transparent 50%),
                        radial-gradient(circle at 80% 80%, rgba(51, 136, 255, 0.05) 0%, transparent 50%),
                        radial-gradient(circle at 40% 20%, rgba(255, 51, 102, 0.05) 0%, transparent 50%);
            animation: bgShift 20s ease-in-out infinite;
        }

        @keyframes bgShift {
            0%, 100% { transform: scale(1) translateY(0); }
            50% { transform: scale(1.1) translateY(-20px); }
        }

        /* Header */
        .header {
            background: rgba(26, 26, 26, 0.95);
            backdrop-filter: blur(20px);
            padding: 1rem 2rem;
            border-bottom: 1px solid var(--border-color);
            position: sticky;
            top: 0;
            z-index: 1000;
            box-shadow: 0 2px 20px rgba(0, 0, 0, 0.5);
        }

        .header-content {
            max-width: 1800px;
            margin: 0 auto;
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        .logo {
            display: flex;
            align-items: center;
            gap: 1rem;
            font-size: 1.5rem;
            font-weight: 700;
            color: var(--accent);
            text-shadow: 0 0 20px rgba(0, 255, 136, 0.5);
        }

        .logo-icon {
            font-size: 2rem;
            animation: pulse 2s ease-in-out infinite;
        }

        @keyframes pulse {
            0%, 100% { transform: scale(1); }
            50% { transform: scale(1.1); }
        }

        .header-stats {
            display: flex;
            gap: 2rem;
            align-items: center;
        }

        .stat-badge {
            background: rgba(0, 255, 136, 0.1);
            border: 1px solid rgba(0, 255, 136, 0.3);
            padding: 0.5rem 1rem;
            border-radius: 8px;
            display: flex;
            align-items: center;
            gap: 0.5rem;
            transition: all 0.3s ease;
        }

        .stat-badge:hover {
            background: rgba(0, 255, 136, 0.2);
            transform: translateY(-2px);
        }

        /* Main Container */
        .container {
            max-width: 1800px;
            margin: 0 auto;
            padding: 2rem;
        }

        /* Global Stats */
        .global-stats {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
            gap: 1.5rem;
            margin-bottom: 3rem;
        }

        .stat-card {
            background: var(--card-bg);
            border: 1px solid var(--border-color);
            border-radius: 12px;
            padding: 1.5rem;
            position: relative;
            overflow: hidden;
            transition: all 0.3s ease;
        }

        .stat-card::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 2px;
            background: linear-gradient(90deg, transparent, var(--accent), transparent);
            animation: scan 3s linear infinite;
        }

        @keyframes scan {
            0% { transform: translateX(-100%); }
            100% { transform: translateX(100%); }
        }

        .stat-card:hover {
            transform: translateY(-5px);
            box-shadow: 0 10px 40px rgba(0, 255, 136, 0.2);
        }

        .stat-card h3 {
            font-size: 0.875rem;
            color: var(--text-secondary);
            margin-bottom: 0.5rem;
            text-transform: uppercase;
            letter-spacing: 1px;
        }

        .stat-value {
            font-size: 2.5rem;
            font-weight: 700;
            color: var(--accent);
            margin-bottom: 0.5rem;
            text-shadow: 0 0 20px currentColor;
        }

        .stat-trend {
            font-size: 0.875rem;
            color: var(--success);
        }

        /* World Map Section */
        .world-map-section {
            background: var(--card-bg);
            border: 1px solid var(--border-color);
            border-radius: 12px;
            padding: 2rem;
            margin-bottom: 3rem;
            position: relative;
            overflow: hidden;
        }

        .map-header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            margin-bottom: 2rem;
        }

        .map-container {
            position: relative;
            height: 500px;
            background: url('data:image/svg+xml;base64,') center/contain no-repeat;
            filter: brightness(0.5);
        }

        .map-point {
            position: absolute;
            width: 12px;
            height: 12px;
            background: var(--accent);
            border-radius: 50%;
            cursor: pointer;
            transition: all 0.3s ease;
            animation: pingAnimation 2s ease-in-out infinite;
        }

        @keyframes pingAnimation {
            0% {
                box-shadow: 0 0 0 0 rgba(0, 255, 136, 0.8);
            }
            70% {
                box-shadow: 0 0 0 20px rgba(0, 255, 136, 0);
            }
            100% {
                box-shadow: 0 0 0 0 rgba(0, 255, 136, 0);
            }
        }

        .map-point.inactive {
            background: var(--danger);
            animation: none;
        }

        .map-point.warning {
            background: var(--warning);
        }

        .map-point:hover {
            transform: scale(1.5);
            z-index: 10;
        }

        .map-tooltip {
            position: absolute;
            background: rgba(26, 26, 26, 0.95);
            border: 1px solid var(--accent);
            border-radius: 8px;
            padding: 1rem;
            display: none;
            z-index: 100;
            backdrop-filter: blur(10px);
            min-width: 200px;
        }

        /* Agents Table */
        .agents-section {
            background: var(--card-bg);
            border: 1px solid var(--border-color);
            border-radius: 12px;
            padding: 2rem;
            margin-bottom: 3rem;
        }

        .section-header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            margin-bottom: 2rem;
        }

        .search-bar {
            background: var(--secondary-bg);
            border: 1px solid var(--border-color);
            border-radius: 8px;
            padding: 0.75rem 1rem;
            color: var(--text-primary);
            width: 300px;
            transition: all 0.3s ease;
        }

        .search-bar:focus {
            outline: none;
            border-color: var(--accent);
            box-shadow: 0 0 20px rgba(0, 255, 136, 0.3);
        }

        .agents-table {
            width: 100%;
            border-collapse: separate;
            border-spacing: 0 0.5rem;
        }

        .agents-table th {
            text-align: left;
            padding: 1rem;
            color: var(--text-secondary);
            font-weight: 500;
            text-transform: uppercase;
            font-size: 0.875rem;
            letter-spacing: 1px;
        }

        .agents-table tr {
            background: var(--secondary-bg);
            transition: all 0.3s ease;
        }

        .agents-table tr:hover {
            background: rgba(0, 255, 136, 0.1);
            transform: translateX(5px);
        }

        .agents-table td {
            padding: 1rem;
            border: none;
        }

        .agents-table td:first-child {
            border-radius: 8px 0 0 8px;
        }

        .agents-table td:last-child {
            border-radius: 0 8px 8px 0;
        }

        .status-badge {
            display: inline-flex;
            align-items: center;
            gap: 0.5rem;
            padding: 0.25rem 0.75rem;
            border-radius: 20px;
            font-size: 0.875rem;
            font-weight: 500;
        }

        .status-badge.active {
            background: rgba(0, 255, 136, 0.2);
            color: var(--success);
            border: 1px solid var(--success);
        }

        .status-badge.inactive {
            background: rgba(255, 51, 102, 0.2);
            color: var(--danger);
            border: 1px solid var(--danger);
        }

        .status-badge.warning {
            background: rgba(255, 170, 51, 0.2);
            color: var(--warning);
            border: 1px solid var(--warning);
        }

        .status-indicator {
            width: 8px;
            height: 8px;
            background: currentColor;
            border-radius: 50%;
            animation: blink 2s ease-in-out infinite;
        }

        @keyframes blink {
            0%, 100% { opacity: 1; }
            50% { opacity: 0.3; }
        }

        .action-buttons {
            display: flex;
            gap: 0.5rem;
        }

        .btn {
            background: transparent;
            border: 1px solid var(--border-color);
            color: var(--text-primary);
            padding: 0.5rem 1rem;
            border-radius: 6px;
            cursor: pointer;
            transition: all 0.3s ease;
            font-size: 0.875rem;
            display: inline-flex;
            align-items: center;
            gap: 0.5rem;
        }

        .btn:hover {
            background: var(--accent);
            color: var(--primary-bg);
            border-color: var(--accent);
            transform: translateY(-2px);
            box-shadow: 0 5px 20px rgba(0, 255, 136, 0.3);
        }

        .btn.danger {
            border-color: var(--danger);
            color: var(--danger);
        }

        .btn.danger:hover {
            background: var(--danger);
            border-color: var(--danger);
            box-shadow: 0 5px 20px rgba(255, 51, 102, 0.3);
        }

        /* Command Center */
        .command-center {
            background: var(--card-bg);
            border: 1px solid var(--border-color);
            border-radius: 12px;
            padding: 2rem;
            margin-bottom: 3rem;
        }

        .command-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            gap: 1.5rem;
            margin-top: 2rem;
        }

        .command-card {
            background: var(--secondary-bg);
            border: 1px solid var(--border-color);
            border-radius: 8px;
            padding: 1.5rem;
            transition: all 0.3s ease;
            cursor: pointer;
        }

        .command-card:hover {
            border-color: var(--accent);
            transform: translateY(-5px);
            box-shadow: 0 10px 30px rgba(0, 255, 136, 0.2);
        }

        .command-icon {
            font-size: 3rem;
            margin-bottom: 1rem;
            display: block;
        }

        .command-title {
            font-size: 1.25rem;
            margin-bottom: 0.5rem;
            color: var(--accent);
        }

        .command-description {
            color: var(--text-secondary);
            font-size: 0.875rem;
        }

        /* Alerts Section */
        .alerts-section {
            background: var(--card-bg);
            border: 1px solid var(--border-color);
            border-radius: 12px;
            padding: 2rem;
        }

        .alert-item {
            background: var(--secondary-bg);
            border-left: 4px solid var(--danger);
            padding: 1rem;
            margin-bottom: 1rem;
            border-radius: 4px;
            display: flex;
            justify-content: space-between;
            align-items: center;
            transition: all 0.3s ease;
        }

        .alert-item.warning {
            border-left-color: var(--warning);
        }

        .alert-item.info {
            border-left-color: var(--info);
        }

        .alert-item:hover {
            transform: translateX(5px);
            background: rgba(255, 51, 102, 0.1);
        }

        .alert-content {
            flex: 1;
        }

        .alert-title {
            font-weight: 600;
            margin-bottom: 0.25rem;
        }

        .alert-time {
            font-size: 0.875rem;
            color: var(--text-secondary);
        }

        /* Floating Action Button */
        .fab {
            position: fixed;
            bottom: 2rem;
            right: 2rem;
            width: 60px;
            height: 60px;
            background: var(--accent);
            color: var(--primary-bg);
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 1.5rem;
            cursor: pointer;
            box-shadow: 0 5px 20px rgba(0, 255, 136, 0.5);
            transition: all 0.3s ease;
            z-index: 1000;
        }

        .fab:hover {
            transform: scale(1.1) rotate(90deg);
            box-shadow: 0 10px 40px rgba(0, 255, 136, 0.7);
        }

        /* Responsive */
        @media (max-width: 768px) {
            .header-stats {
                display: none;
            }

            .global-stats {
                grid-template-columns: 1fr;
            }

            .search-bar {
                width: 100%;
                margin-top: 1rem;
            }

            .section-header {
                flex-direction: column;
                align-items: flex-start;
            }

            .agents-table {
                font-size: 0.875rem;
            }

            .action-buttons {
                flex-direction: column;
            }
        }
    </style>
</head>
<body>
    <div class="bg-animation"></div>

    <!-- Header -->
    <header class="header">
        <div class="header-content">
            <div class="logo">
                <span class="logo-icon">🛡️</span>
                AI SENTINEL-X SUPER ADMIN
            </div>
            <div class="header-stats">
                <div class="stat-badge">
                    <span>🌍</span>
                    <span id="totalAgents">2,847</span> Global Agents
                </div>
                <div class="stat-badge">
                    <span>💰</span>
                    <span id="mrr">$487,293</span> MRR
                </div>
                <div class="stat-badge">
                    <span>⚡</span>
                    <span id="systemLoad">12.4M</span> Threats/Hour
                </div>
            </div>
        </div>
    </header>

    <!-- Main Container -->
    <div class="container">
        <!-- Global Stats -->
        <div class="global-stats">
            <div class="stat-card">
                <h3>Active Agents</h3>
                <div class="stat-value" id="activeAgents">2,483</div>
                <div class="stat-trend">↑ 12% from last month</div>
            </div>
            <div class="stat-card">
                <h3>Total Customers</h3>
                <div class="stat-value" id="totalCustomers">847</div>
                <div class="stat-trend">↑ 23% from last month</div>
            </div>
            <div class="stat-card">
                <h3>Threats Blocked</h3>
                <div class="stat-value" id="threatsBlocked">14.2M</div>
                <div class="stat-trend">Today's total</div>
            </div>
            <div class="stat-card">
                <h3>Uptime</h3>
                <div class="stat-value">99.98%</div>
                <div class="stat-trend">Last 30 days</div>
            </div>
            <div class="stat-card">
                <h3>Overdue Accounts</h3>
                <div class="stat-value" style="color: var(--warning);">47</div>
                <div class="stat-trend" style="color: var(--warning);">Requires attention</div>
            </div>
            <div class="stat-card">
                <h3>Support Tickets</h3>
                <div class="stat-value" style="color: var(--info);">23</div>
                <div class="stat-trend">8 critical</div>
            </div>
        </div>

        <!-- World Map -->
        <div class="world-map-section">
            <div class="map-header">
                <h2>🌍 Global Agent Distribution</h2>
                <div class="stat-badge">
                    <span style="color: var(--success);">●</span> Active
                    <span style="color: var(--warning);">●</span> Warning
                    <span style="color: var(--danger);">●</span> Inactive
                </div>
            </div>
            <div class="map-container" id="worldMap">
                <!-- Map points will be dynamically generated -->
            </div>
            <div class="map-tooltip" id="mapTooltip"></div>
        </div>

        <!-- Command Center -->
        <div class="command-center">
            <h2>🎮 Command Center</h2>
            <div class="command-grid">
                <div class="command-card" onclick="massUpdate()">
                    <span class="command-icon">🔄</span>
                    <div class="command-title">Mass Update</div>
                    <div class="command-description">Push updates to all agents globally</div>
                </div>
                <div class="command-card" onclick="emergencyShutdown()">
                    <span class="command-icon">🚨</span>
                    <div class="command-title">Emergency Shutdown</div>
                    <div class="command-description">Instantly disable all agents</div>
                </div>
                <div class="command-card" onclick="generateReport()">
                    <span class="command-icon">📊</span>
                    <div class="command-title">Generate Reports</div>
                    <div class="command-description">Financial & operational analytics</div>
                </div>
                <div class="command-card" onclick="supportDashboard()">
                    <span class="command-icon">💬</span>
                    <div class="command-title">Support Dashboard</div>
                    <div class="command-description">View and manage tickets</div>
                </div>
            </div>
        </div>

        <!-- Agents Table -->
        <div class="agents-section">
            <div class="section-header">
                <h2>🤖 Agent Management</h2>
                <input type="text" class="search-bar" placeholder="Search by customer, ID, or location..." onkeyup="filterAgents(this.value)">
            </div>
            <table class="agents-table">
                <thead>
                    <tr>
                        <th>Agent ID</th>
                        <th>Customer</th>
                        <th>Location</th>
                        <th>Status</th>
                        <th>Sub-Agents</th>
                        <th>Threats/Hour</th>
                        <th>License</th>
                        <th>Actions</th>
                    </tr>
                </thead>
                <tbody id="agentsTableBody">
                    <!-- Table rows will be dynamically generated -->
                </tbody>
            </table>
        </div>

        <!-- Alerts Section -->
        <div class="alerts-section">
            <h2>🚨 System Alerts</h2>
            <div id="alertsList">
                <!-- Alerts will be dynamically generated -->
            </div>
        </div>
    </div>

    <!-- Floating Action Button -->
    <div class="fab" onclick="openChat()">💬</div>

    <script>
        // Sample data
        const agents = [
            { id: 'SX-001', customer: 'TechCorp Inc', location: 'New York, US', status: 'active', subAgents: 9, threats: '12.4K', license: 'Enterprise', billing: 'current' },
            { id: 'SX-002', customer: 'CyberShield LLC', location: 'London, UK', status: 'active', subAgents: 9, threats: '8.7K', license: 'Enterprise', billing: 'current' },
            { id: 'SX-003', customer: 'DataFort Systems', location: 'Tokyo, JP', status: 'warning', subAgents: 9, threats: '15.2K', license: 'Business', billing: 'overdue' },
            { id: 'SX-004', customer: 'SecureNet Pro', location: 'Sydney, AU', status: 'active', subAgents: 9, threats: '6.3K', license: 'Essential', billing: 'current' },
            { id: 'SX-005', customer: 'CloudArmor', location: 'Berlin, DE', status: 'inactive', subAgents: 0, threats: '0', license: 'Enterprise', billing: 'suspended' },
            { id: 'SX-006', customer: 'InfoGuard Plus', location: 'Toronto, CA', status: 'active', subAgents: 9, threats: '9.8K', license: 'Business', billing: 'current' },
            { id: 'SX-007', customer: 'NetDefender', location: 'Singapore, SG', status: 'active', subAgents: 9, threats: '11.2K', license: 'Enterprise', billing: 'current' },
            { id: 'SX-008', customer: 'CryptoShield', location: 'Dubai, AE', status: 'warning', subAgents: 9, threats: '7.5K', license: 'Business', billing: 'overdue' }
        ];

        const alerts = [
            { type: 'danger', title: 'Critical: 5 agents offline in EU region', time: '2 minutes ago' },
            { type: 'warning', title: 'Warning: 47 overdue accounts require attention', time: '15 minutes ago' },
            { type: 'info', title: 'Info: Scheduled maintenance in 2 hours', time: '1 hour ago' },
            { type: 'danger', title: 'Critical: DDoS attack detected on SX-003', time: '3 hours ago' }
        ];

        // Initialize dashboard
        function initDashboard() {
            renderAgentsTable();
            renderAlerts();
            renderWorldMap();
            animateNumbers();
        }

        // Render agents table
        function renderAgentsTable() {
            const tbody = document.getElementById('agentsTableBody');
            tbody.innerHTML = agents.map(agent => `
                <tr>
                    <td><strong>${agent.id}</strong></td>
                    <td>${agent.customer}</td>
                    <td>${agent.location}</td>
                    <td>
                        <span class="status-badge ${agent.status}">
                            <span class="status-indicator"></span>
                            ${agent.status.charAt(0).toUpperCase() + agent.status.slice(1)}
                        </span>
                    </td>
                    <td>${agent.subAgents}/9</td>
                    <td>${agent.threats}</td>
                    <td>${agent.license}</td>
                    <td>
                        <div class="action-buttons">
                            <button class="btn" onclick="viewAgent('${agent.id}')">View</button>
                            <button class="btn ${agent.status === 'active' ? 'danger' : ''}" 
                                    onclick="toggleAgent('${agent.id}')">
                                ${agent.status === 'active' ? 'Disable' : 'Enable'}
                            </button>
                        </div>
                    </td>
                </tr>
            `).join('');
        }

        // Render alerts
        function renderAlerts() {
            const alertsList = document.getElementById('alertsList');
            alertsList.innerHTML = alerts.map(alert => `
                <div class="alert-item ${alert.type}">
                    <div class="alert-content">
                        <div class="alert-title">${alert.title}</div>
                        <div class="alert-time">${alert.time}</div>
                    </div>
                    <button class="btn" onclick="dismissAlert(this)">Dismiss</button>
                </div>
            `).join('');
        }

        // Render world map points
        function renderWorldMap() {
            const mapContainer = document.getElementById('worldMap');
            const locations = [
                { x: '20%', y: '35%', status: 'active', info: 'New York: 247 agents' },
                { x: '45%', y: '25%', status: 'active', info: 'London: 189 agents' },
                { x: '75%', y: '40%', status: 'warning', info: 'Tokyo: 156 agents (3 warnings)' },
                { x: '80%', y: '75%', status: 'active', info: 'Sydney: 98 agents' },
                { x: '50%', y: '30%', status: 'inactive', info: 'Berlin: 45 agents (OFFLINE)' },
                { x: '25%', y: '25%', status: 'active', info: 'Toronto: 123 agents' },
                { x: '70%', y: '55%', status: 'active', info: 'Singapore: 201 agents' },
                { x: '60%', y: '45%', status: 'warning', info: 'Dubai: 87 agents (2 warnings)' }
            ];

            locations.forEach(loc => {
                const point = document.createElement('div');
                point.className = `map-point ${loc.status}`;
                point.style.left = loc.x;
                point.style.top = loc.y;
                point.setAttribute('data-info', loc.info);
                point.onmouseenter = (e) => showTooltip(e, loc.info);
                point.onmouseleave = hideTooltip;
                mapContainer.appendChild(point);
            });
        }

        // Tooltip functions
        function showTooltip(e, text) {
            const tooltip = document.getElementById('mapTooltip');
            tooltip.innerHTML = text;
            tooltip.style.display = 'block';
            tooltip.style.left = e.target.offsetLeft + 20 + 'px';
            tooltip.style.top = e.target.offsetTop - 10 + 'px';
        }

        function hideTooltip() {
            document.getElementById('mapTooltip').style.display = 'none';
        }

        // Filter agents
        function filterAgents(searchTerm) {
            const rows = document.querySelectorAll('.agents-table tbody tr');
            rows.forEach(row => {
                const text = row.textContent.toLowerCase();
                row.style.display = text.includes(searchTerm.toLowerCase()) ? '' : 'none';
            });
        }

        // Action functions
        function viewAgent(id) {
            alert(`Opening detailed view for agent ${id}...`);
        }

        function toggleAgent(id) {
            const agent = agents.find(a => a.id === id);
            if (agent) {
                agent.status = agent.status === 'active' ? 'inactive' : 'active';
                renderAgentsTable();
                alert(`Agent ${id} has been ${agent.status === 'active' ? 'enabled' : 'disabled'}`);
            }
        }

        function dismissAlert(btn) {
            btn.parentElement.style.animation = 'fadeOut 0.3s ease';
            setTimeout(() => btn.parentElement.remove(), 300);
        }

        function massUpdate() {
            if (confirm('Push update to all 2,847 agents globally?')) {
                alert('Update initiated. Estimated completion: 15 minutes');
            }
        }

        function emergencyShutdown() {
            if (confirm('⚠️ EMERGENCY SHUTDOWN ⚠️\n\nThis will disable ALL agents globally. Continue?')) {
                if (confirm('Are you absolutely sure? This action cannot be undone immediately.')) {
                    alert('Emergency shutdown initiated. All agents disabled.');
                }
            }
        }

        function generateReport() {
            alert('Generating comprehensive report...\n\n• Financial Analytics\n• Threat Statistics\n• Customer Health\n• System Performance');
        }

        function supportDashboard() {
            alert('Opening support dashboard with 23 active tickets...');
        }

        function openChat() {
            alert('Opening AI-powered support chat...');
        }

        // Animate numbers on load
        function animateNumbers() {
            const elements = [
                { id: 'totalAgents', end: 2847 },
                { id: 'activeAgents', end: 2483 },
                { id: 'totalCustomers', end: 847 },
                { id: 'threatsBlocked', end: 14.2, suffix: 'M' }
            ];

            elements.forEach(el => {
                let start = 0;
                const increment = el.end / 50;
                const timer = setInterval(() => {
                    start += increment;
                    if (start >= el.end) {
                        start = el.end;
                        clearInterval(timer);
                    }
                    document.getElementById(el.id).textContent = 
                        el.suffix ? start.toFixed(1) + el.suffix : Math.floor(start).toLocaleString();
                }, 30);
            });
        }

        // Add fade out animation
        const style = document.createElement('style');
        style.textContent = `
            @keyframes fadeOut {
                to {
                    opacity: 0;
                    transform: translateX(-100%);
                }
            }
        `;
        document.head.appendChild(style);

        // Auto-refresh data every 30 seconds
        setInterval(() => {
            // Update random values to simulate real-time data
            document.getElementById('systemLoad').textContent = 
                (Math.random() * 5 + 10).toFixed(1) + 'M';
            
            const threatCount = document.getElementById('threatsBlocked');
            const current = parseFloat(threatCount.textContent);
            threatCount.textContent = (current + Math.random() * 0.1).toFixed(1) + 'M';
        }, 30000);

        // Initialize on load
        window.onload = initDashboard;
    </script>
</body>
</html>
